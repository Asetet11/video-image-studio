{
  "name": "luna",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"wait-on tcp:5173 && electron .\"",
    "build:renderer": "vite build",
    "prebuild": "node electron/copy-ffmpeg-core.cjs",
    "installer:win": "npm run build:renderer && electron-builder --win nsis --publish always",
    "icons:gen": "node electron/generate-icons.cjs"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.7",
    "@ffmpeg/util": "^0.12.1",
    "electron-squirrel-startup": "^1.0.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.468.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "electron": "^30.0.0",
    "electron-builder": "^24.13.3",
    "icon-gen": "^3.0.1",
    "png-to-ico": "^2.1.8",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.4.5",
    "vite": "^5.2.0",
    "@vitejs/plugin-react": "^4.3.0",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.luna.editor",
    "productName": "Luna",
    "directories": {
      "buildResources": "build"
    },
    "files": [
      "dist/**",
      "electron/**",
      "package.json",
      "public/**"
    ],
    "asarUnpack": [
      "**/*.wasm",
      "**/*.worker.js",
      "**/ffmpeg-core*"
    ],
    "artifactName": "Luna-Setup-${version}.exe",
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico",
      "publisherName": "Luna"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Luna"
    },
    "mac": {
      "category": "public.app-category.video",
      "icon": "build/icon.icns",
      "target": [
        "dmg"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Video",
      "icon": "build/icon.png"
    },
    "publish": [
      {
        "provider": "github"
      }
    ]
  }
}
